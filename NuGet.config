<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Add your Artifactory source here -->
    <!-- <add key="Artifactory" value="https://your-artifactory-url/api/nuget/nuget-virtual" /> -->
    
    <!-- Keep nuget.org as fallback if your Artifactory proxies it -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  
  <packageSourceMapping>
    <!-- Map all packages to your configured sources -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
  
  <config>
    <!-- Allow NuGet to skip missing package versions and use available ones -->
    <add key="dependencyVersion" value="HighestMinor" />
  </config>
</configuration>
